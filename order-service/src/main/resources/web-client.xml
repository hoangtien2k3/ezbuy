<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="webClientFactory" class="com.ezbuy.sme.framework.client.WebClientFactory">
        <property name="webClients">
            <list>
                <ref bean="profileProperties"></ref>
                <ref bean="orderProperties"></ref>
                <ref bean="productProperties"></ref>
                <ref bean="paymentProperties"></ref>
                <ref bean="settingProperties"></ref>
                <ref bean="cartProperties"></ref>
                <ref bean="cmProperties"></ref>
                <ref bean="authProperties"></ref>
                <ref bean="pricingProperties"></ref>
                <ref bean="cmPortalProperties"></ref>
                <ref bean="orderV2Properties"></ref>
                <ref bean="gatewayProperties"></ref>
            </list>
        </property>
    </bean>
</beans>
